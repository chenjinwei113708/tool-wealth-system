{
  "name": "wealth_system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "cross-env GENERATE_SOURCEMAP=false craco build",
    "eject": "react-scripts eject",
    "buildserver": "tsc -p ./app/tsconfig.json && tsc-alias -p ./app/tsconfig.json",
