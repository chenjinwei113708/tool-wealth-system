    "prod": "node app-js/app.js",
    "server": "cross-env NODE_ENV=production nodemon --exec ts-node --project ./app/tsconfig.json -r tsconfig-paths/register app/app.ts",
    "serverd": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development nodemon --exec ts-node --project ./app/tsconfig.json -r tsconfig-paths/register app/app.ts"
  },
