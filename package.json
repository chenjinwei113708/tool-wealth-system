    "koa-body": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "log4js": "^5.2.2",
    "md5": "^2.3.0",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "nodemon": "^1.19.3",
